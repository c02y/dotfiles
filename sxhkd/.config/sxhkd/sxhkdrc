	tdrop -ma -w 100% -h 90% alacritty
super + k
	jumpapp kitty
super + e
	tdrop -ma -w 100% -h 100% emacsclient -c
super + f
	jumpapp firefox
super + c
	jumpapp google-chrome-stable
super + a
	fsearch
