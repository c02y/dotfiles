super + Escape
	pkill -USR1 -x sxhkd && notify-send "[sxhkd] reloaded"
F12
	$HOME/.bin/tdrop -ma -w 100% -h 90% alacritty -e $HOME/.bin/t
super + k
	$HOME/.bin/jumpapp kitty
super + e
	$HOME/.bin/tdrop -ma -w 100% -h 100% -s -f emacsclient -c
super + f
	$HOME/.bin/jumpapp firefox
super + c
	$HOME/.bin/jumpapp google-chrome-stable
