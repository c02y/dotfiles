[Appearance]
AntiAliasFonts=true
BoldIntense=false
ColorScheme=GreenOnBlack
Font=Menlo,12,-1,5,50,0,0,0,0,0

[Cursor Options]
CursorShape=1

[General]
Command=/usr/bin/tmux
LocalTabTitleFormat=%D
Name=Shell
Parent=FALLBACK/
StartInCurrentSessionDir=true

[Interaction Options]
AutoCopySelectedText=true
MouseWheelZoomEnabled=false
TripleClickMode=0

[Keyboard]
KeyBindings=default

[Scrolling]
HistoryMode=2
HistorySize=99999
ScrollBarPosition=2

[Terminal Features]
BlinkingCursorEnabled=true
